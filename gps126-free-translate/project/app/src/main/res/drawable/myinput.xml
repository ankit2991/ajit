<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >

            <stroke android:width="1dip" android:color="#ED555555" />
            <gradient android:angle="90" android:startColor="#B7292929" android:endColor="#BE353535" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >

            <stroke android:width="1dip" android:color="#ED555555" />
            <gradient android:angle="90" android:startColor="#B7292929" android:endColor="#BE353535" />
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >

            <stroke android:width="1dip" android:color="#61555555" />
            <gradient android:angle="90" android:startColor="#95292929" android:endColor="#99353535" />
        </shape>
    </item>
</selector>